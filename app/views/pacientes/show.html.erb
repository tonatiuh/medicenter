<%- model_class = @paciente.class -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:nombre) %>:</strong></dt>
  <dd><%= @paciente.nombre %></dd>
  <dt><strong><%= model_class.human_attribute_name(:telefono) %>:</strong></dt>
  <dd><%= @paciente.telefono %></dd>
  <dt><strong><%= model_class.human_attribute_name(:lugar_de_nacimiento) %>:</strong></dt>
  <dd><%= @paciente.lugar_de_nacimiento %></dd>
  <dt><strong><%= model_class.human_attribute_name(:antecedentes_patologicos) %>:</strong></dt>
  <dd><%= @paciente.antecedentes_patologicos %></dd>
  <dt><strong><%= model_class.human_attribute_name(:antecedentes_no_patologicos) %>:</strong></dt>
  <dd><%= @paciente.antecedentes_no_patologicos %></dd>
  <dt><strong><%= model_class.human_attribute_name(:antecedentes_heredofamiliares) %>:</strong></dt>
  <dd><%= @paciente.antecedentes_heredofamiliares %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              pacientes_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_paciente_path(@paciente), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              paciente_path(@paciente),
              :method => 'delete',
              :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')),
              :class => 'btn btn-danger' %>
</div>
