<%- model_class = @examan.class -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:titulo) %>:</strong></dt>
  <dd><%= @examan.titulo %></dd>
</dl>

<%= render :partial => 'reconocimiento', :locals => {:reconocimiento => @examan.reconocimiento } %>


<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              examen_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_examan_path(@examan), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              examan_path(@examan),
              :method => 'delete',
              :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')),
              :class => 'btn btn-danger' %>
</div>
